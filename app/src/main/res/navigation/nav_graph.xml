<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.siri_hate.findhelp.view.fragments.LoginFragment"
        android:label="login_fragment"
        tools:layout="@layout/fragment_login_page" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_userPageFragment"
            app:destination="@id/userPageFragment" />
        <action
            android:id="@+id/action_loginFragment_to_moderatorPageFragment"
            app:destination="@id/moderatorPageFragment" />
        <action
            android:id="@+id/action_loginFragment_to_organizerPageFragment"
            app:destination="@id/organizerPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.siri_hate.findhelp.view.fragments.RegisterFragment"
        android:label="register_fragment"
        tools:layout="@layout/fragment_register_page" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/userPageFragment"
        android:name="com.siri_hate.findhelp.view.fragments.UserPageFragment"
        android:label="user_page_fragment"
        tools:layout="@layout/fragment_user_page" >
        <action
            android:id="@+id/action_userPageFragment_to_vacancyCardFragment"
            app:destination="@id/vacancyCardFragment" />
        <action
            android:id="@+id/action_userPageFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
        <action
            android:id="@+id/action_userPageFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.siri_hate.findhelp.view.fragments.UserProfileFragment"
        android:label="user_profile_fragment"
        tools:layout="@layout/fragment_user_profile" >
        <action
            android:id="@+id/action_userProfileFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_userProfileFragment_to_userPageFragment"
            app:destination="@id/userPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/moderatorPageFragment"
        android:name="com.siri_hate.findhelp.view.fragments.ModeratorPageFragment"
        android:label="moderator_page_fragment"
        tools:layout="@layout/fragment_moderator_page" >
        <action
            android:id="@+id/action_moderatorPageFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_moderatorPageFragment_to_vacancyCardFragment"
            app:destination="@id/vacancyCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/organizerPageFragment"
        android:name="com.siri_hate.findhelp.view.fragments.OrganizerPageFragment"
        android:label="organizer_page_fragment"
        tools:layout="@layout/fragment_organizer_page" >
        <action
            android:id="@+id/action_organizerPageFragment_to_createVacancyMainFragment"
            app:destination="@id/createVacancyMainFragment" />
        <action
            android:id="@+id/action_organizerPageFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_organizerPageFragment_to_vacancyCardFragment"
            app:destination="@id/vacancyCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/createVacancyMainFragment"
        android:name="com.siri_hate.findhelp.view.fragments.CreateVacancyMainFragment"
        android:label="create_vacancy_main_fragment"
        tools:layout="@layout/fragment_create_vacancy_info" >
        <action
            android:id="@+id/action_createVacancyMainFragment_to_organizerPageFragment"
            app:destination="@id/organizerPageFragment" />
        <action
            android:id="@+id/action_createVacancyMainFragment_to_createVacancySecondFragment"
            app:destination="@id/createVacancySecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/createVacancySecondFragment"
        android:name="com.siri_hate.findhelp.view.fragments.CreateVacancySecondFragment"
        android:label="create_vacancy_second_fragment"
        tools:layout="@layout/fragment_create_vacancy_skills" >
        <action
            android:id="@+id/action_createVacancySecondFragment_to_organizerPageFragment"
            app:destination="@id/organizerPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/vacancyCardFragment"
        android:name="com.siri_hate.findhelp.view.fragments.VacancyCardFragment"
        android:label="vacancy_card_fragment"
        tools:layout="@layout/fragment_vacancy_card" >
        <action
            android:id="@+id/action_vacancyCardFragment_to_userPageFragment"
            app:destination="@id/userPageFragment" />
        <action
            android:id="@+id/action_vacancyCardFragment_to_organizerPageFragment"
            app:destination="@id/organizerPageFragment" />
        <action
            android:id="@+id/action_vacancyCardFragment_to_moderatorPageFragment"
            app:destination="@id/moderatorPageFragment" />
        <action
            android:id="@+id/action_vacancyCardFragment_to_editVacancyMainFragment"
            app:destination="@id/editVacancyMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/editVacancyMainFragment"
        android:name="com.siri_hate.findhelp.view.fragments.EditVacancyMainFragment"
        android:label="edit_vacancy_main_fragment"
        tools:layout="@layout/fragment_edit_vacancy_skills" >
        <action
            android:id="@+id/action_editVacancyMainFragment_to_editVacancySecondFragment"
            app:destination="@id/editVacancySecondFragment" />
        <action
            android:id="@+id/action_editVacancyMainFragment_to_vacancyCardFragment"
            app:destination="@id/vacancyCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/editVacancySecondFragment"
        android:name="com.siri_hate.findhelp.view.fragments.EditVacancySecondFragment"
        android:label="edit_vacancy_second_fragment"
        tools:layout="@layout/fragment_edit_vacancy_info" >
        <action
            android:id="@+id/action_editVacancySecondFragment_to_vacancyCardFragment"
            app:destination="@id/vacancyCardFragment" />
    </fragment>
</navigation>